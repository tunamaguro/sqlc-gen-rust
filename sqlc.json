{
    "version": "2",
    "plugins": [
        {
            "name": "sqlc-gen-rust",
            "wasm": {
                "url": "file://target/wasm32-wasip1/debug/sqlc-gen-rust.wasm",
                "sha256": "$WASM_SHA256"
            }
        }
    ],
    "sql": [
        {
            "schema": "examples/e-commerce/schema.sql",
            "queries": "examples/e-commerce/queries.sql",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/e-commerce/src",
                    "options": {
                        "output": "tokio_query.rs",
                        "db_crate": "tokio-postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/e-commerce/src",
                    "options": {
                        "output": "postgres_query.rs",
                        "db_crate": "postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/e-commerce/src",
                    "options": {
                        "output": "deadpool_query.rs",
                        "db_crate": "deadpool-postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/e-commerce/src",
                    "options": {
                        "output": "sqlx_query.rs",
                        "db_crate": "sqlx-postgres",
                        "overrides": [
                            {
                                "db_type": "timestamptz",
                                "rs_type": "chrono::DateTime<chrono::Utc>"
                            },
                            {
                                "column": "users.created_at",
                                "rs_type": "chrono::DateTime<chrono::Local>"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "schema": "examples/authors/tokio-postgres/schema.sql",
            "queries": "examples/authors/tokio-postgres/queries.sql",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/authors/tokio-postgres/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "tokio-postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/authors/deadpool-postgres/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "deadpool-postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/authors/postgres/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/authors/sqlx-postgres/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "sqlx-postgres"
                    }
                }
            ]
        },
        {
            "schema": "examples/authors/sqlx-mysql/schema.sql",
            "queries": "examples/authors/sqlx-mysql/queries.sql",
            "engine": "mysql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/authors/sqlx-mysql/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "sqlx-mysql"
                    }
                }
            ]
        },
        {
            "schema": "examples/authors/sqlx-sqlite/schema.sql",
            "queries": "examples/authors/sqlx-sqlite/queries.sql",
            "engine": "sqlite",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/authors/sqlx-sqlite/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "sqlx-sqlite"
                    }
                }
            ]
        },
        {
            "schema": "examples/jets/schema.sql",
            "queries": "examples/jets/queries.sql",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/jets/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "deadpool-postgres"
                    }
                }
            ]
        },
        {
            "schema": "examples/ondeck/schema",
            "queries": "examples/ondeck/query",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/ondeck/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "tokio-postgres"
                    }
                }
            ]
        },
        {
            "schema": "examples/copyfrom/schema.sql",
            "queries": "examples/copyfrom/queries.sql",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/copyfrom/src",
                    "options": {
                        "output": "tokio_query.rs",
                        "db_crate": "tokio-postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/copyfrom/src",
                    "options": {
                        "output": "postgres_query.rs",
                        "db_crate": "postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/copyfrom/src",
                    "options": {
                        "output": "deadpool_query.rs",
                        "db_crate": "deadpool-postgres"
                    }
                },
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/copyfrom/src",
                    "options": {
                        "output": "sqlx_query.rs",
                        "db_crate": "sqlx-postgres"
                    }
                }
            ]
        },
        {
            "schema": "examples/type-mapping/tokio-postgres/schema.sql",
            "queries": "examples/type-mapping/tokio-postgres/queries.sql",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/type-mapping/tokio-postgres/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "tokio-postgres",
                        "overrides": [
                            {
                                "db_type": "date",
                                "rs_type": "chrono::NaiveDate"
                            },
                            {
                                "db_type": "pg_catalog.time",
                                "rs_type": "chrono::NaiveTime"
                            },
                            {
                                "db_type": "complex",
                                "rs_type": "crate::Complex",
                                "copy_cheap": true
                            }
                        ]
                    }
                }
            ]
        },
        {
            "schema": "examples/type-mapping/sqlx-postgres/schema.sql",
            "queries": "examples/type-mapping/sqlx-postgres/queries.sql",
            "engine": "postgresql",
            "codegen": [
                {
                    "plugin": "sqlc-gen-rust",
                    "out": "examples/type-mapping/sqlx-postgres/src",
                    "options": {
                        "output": "queries.rs",
                        "db_crate": "sqlx-postgres",
                        "overrides": [
                            {
                                "db_type": "timestamptz",
                                "rs_type": "chrono::DateTime<chrono::Utc>"
                            },
                            {
                                "db_type": "pg_catalog.timestamp",
                                "rs_type": "chrono::NaiveDateTime"
                            },
                            {
                                "db_type": "date",
                                "rs_type": "chrono::NaiveDate"
                            },
                            {
                                "db_type": "pg_catalog.time",
                                "rs_type": "chrono::NaiveTime"
                            },
                            {
                                "db_type": "complex",
                                "rs_type": "crate::Complex",
                                "copy_cheap": true
                            }
                        ]
                    }
                }
            ]
        }
    ]
}